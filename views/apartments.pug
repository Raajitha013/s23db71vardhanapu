extends layout

block content
  h1= title
  - var app= results
  div(class="container-fluid")
  div(class="row")
   each z,index  in app
    div(class="col-sm-2" onclick =`buy(${index})`) #{z.apartment_location}, #{z.apartment_size}, #{z.apartment_rent}
    script.
        function buy(a)
        {
            alert(`You purchased Item ${a+1}`)
        }
